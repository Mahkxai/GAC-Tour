            android:configChanges="orientation|screenSize|screenLayout"
            android:description="@string/description_map_viewport_animation"
            android:exported="true"
            android:label="@string/activity_map_viewport_animation"
            android:parentActivityName="com.example.gac_tour.ExampleOverviewActivity" >
            <meta-data
                android:name="@string/category"
                android:value="@string/category_camera" />
        </activity>
        <activity
            android:name="com.example.gac_tour.examples.basic.MultiDisplayActivity"
            android:configChanges="orientation|screenSize|screenLayout"
            android:description="@string/description_multiple_display"
            android:exported="true"
            android:label="@string/activity_multiple_display"
            android:parentActivityName="com.example.gac_tour.ExampleOverviewActivity" >
            <meta-data
                android:name="@string/category"
                android:value="@string/category_basic" />
        </activity>

